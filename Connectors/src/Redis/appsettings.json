{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "Redis": {
    "Client": {
      "ConnectRetry": 3
    }
  },
  "Management": {
    "Endpoints": {
      "Path": "/cloudfoundryapplication",
      "Cloudfoundry": {
        "ValidateCertificates": false
      }
    }
  }
}
